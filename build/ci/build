#!/bin/sh

echo "**********************************"
echo "* Building container-suseconnect *"
echo "**********************************"

CS_BUILD_DIR=$(pwd)/build/container-suseconnect
PROJECT=github.com/SUSE/container-suseconnect

rm -rf $CS_BUILD_DIR
mkdir -p $CS_BUILD_DIR
GOBIN=$CS_BUILD_DIR go install -ldflags='-w -s' -a -v ./...
